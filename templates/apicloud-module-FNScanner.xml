    <variable name="path" expression="snakeCase(&quot;path&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="200" expression="snakeCase(&quot;200&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="2001" expression="snakeCase(&quot;200&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="FNScanner.switchLight" value="FNScanner.switchLight({&#10;     status: '$off$'&#10;});" description="打开/关闭闪光灯" toReformat="true" toShortenFQNames="true">
    <variable name="off" expression="snakeCase(&quot;off&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>