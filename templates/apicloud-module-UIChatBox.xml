      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="UIChatBox.value" value="UIChatBox.value({&#10;    msg:'$msg$'&#10;},function(ret,err){&#10;&#10;});" description="获取或设置聊天输入框的内容" toReformat="true" toShortenFQNames="true">
