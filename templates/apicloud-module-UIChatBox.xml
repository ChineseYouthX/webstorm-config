      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="UIChatBox.insertValue" value="UIChatBox.insertValue({&#10;    index:$0$,&#10;    msg:'$msg$'&#10;},function(ret,err){&#10;&#10;});" description="向聊天输入框的指定位置插入内容" toReformat="true" toShortenFQNames="true">
