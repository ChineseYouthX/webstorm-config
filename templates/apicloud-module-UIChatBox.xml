    </context>
  </template>
  <template name="UIChatBox.setPlaceholder" value="UIChatBox.setPlaceholder({&#10;    placeholder:'$msg$'&#10;});" description="重设聊天输入框的占位提示文本" toReformat="true" toShortenFQNames="true">
    <variable name="msg" expression="snakeCase(&quot;placeholder&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>