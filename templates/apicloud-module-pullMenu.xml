<templateSet group="apicloud-module-pullMenu">
  <template name="pullMenu.open" value="pullMenu.open({    &#10;&#9;type:'$up$',&#10;&#9;h:$300$,&#10;&#9;btnWidth:$60$,&#10;&#9;alpha:$alpha$,&#10;&#9;bgColor:'$bgColor$',&#10;    btnArray:$btnArray$&#10;}, function(ret, err){&#10;&#10;});" description="打开拖拉菜单" toReformat="true" toShortenFQNames="true">
    <variable name="up" expression="snakeCase(&quot;up&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="300" expression="snakeCase(&quot;300&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="60" expression="snakeCase(&quot;60&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="alpha" expression="snakeCase(&quot;alpha&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="bgColor" expression="snakeCase(&quot;bgcolor&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="btnArray" expression="snakeCase(&quot;btnarray&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="pullMenu.setSelected" value="pullMenu.setSelected({    &#10;&#9;index: $0$,&#10;&#9;selected: $true$&#10;});" description="设置按钮选中/取消状态" toReformat="true" toShortenFQNames="true">
    <variable name="0" expression="snakeCase(&quot;0&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="true" expression="snakeCase(&quot;true&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="pullMenu.hide" value="pullMenu.hide();" description="隐藏菜单" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="pullMenu.show" value="pullMenu.show();" description="显示菜单" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="pullMenu.close" value="pullMenu.close();" description="关闭菜单" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>