  <template name="qq.shareVideo" value="qq.shareVideo(({&#10;    url: '$url$',&#10;    title: '$title$',&#10;    description: '$description$',&#10;    imgUrl: '$imgpath$' &#10;},function(ret, err) {&#10; &#10;});" description="分享视频到空间/好友" toReformat="true" toShortenFQNames="true">
    <variable name="url" expression="snakeCase(&quot;url&quot;)" defaultValue="" alwaysStopAt="true" />
