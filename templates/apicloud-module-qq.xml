    <variable name="description" expression="snakeCase(&quot;description&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="imgpath" expression="snakeCase(&quot;imgpath&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="qq.shareNews" value="qq.shareNews(({&#10;    url: '$url$',&#10;    title: '$title$',&#10;    description: '$description$',&#10;    imgUrl: '$imgpath$' &#10;},function(ret, err) {&#10; &#10;});" description="分享新闻到空间/好友" toReformat="true" toShortenFQNames="true">
