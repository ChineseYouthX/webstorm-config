<templateSet group="apicloud-module-qq">
  <template name="qq.installed" value="qq.installed(function(ret,err){&#10;    &#10;});" description="判断当前设备是否安装了QQ客户端" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="qq.login" value="qq.login(function(ret, err) {&#10; &#10;});" description="登陆qq" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="qq.logout" value="qq.logout(function(ret, err) {&#10; &#10;});" description="登出qq" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="qq.getUserInfo" value="qq.getUserInfo(function(ret, err) {&#10; &#10;});" description="获取用户信息" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="qq.shareText" value="qq.shareText(({&#10;    text: '$text$'&#10;},function(ret, err) {&#10; &#10;});" description="分享纯文本到好友" toReformat="true" toShortenFQNames="true">
