      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="qq.shareImage" value="qq.shareImage(({&#10;    title: '$title$',&#10;    description: '$description$',&#10;    imgPath: '$imgpath$' &#10;},function(ret, err) {&#10; &#10;});" description="分享图片（本地）到好友" toReformat="true" toShortenFQNames="true">
