  <template name="qq.shareMusic" value="qq.shareMusic(({&#10;    url: '$url$',&#10;    title: '$title$',&#10;    description: '$description$',&#10;    imgUrl: '$imgpath$' &#10;},function(ret, err) {&#10; &#10;});" description="分享音乐到空间/好友" toReformat="true" toShortenFQNames="true">
