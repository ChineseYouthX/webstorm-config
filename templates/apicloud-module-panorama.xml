<templateSet group="apicloud-module-panorama">
  <template name="panorama.open" value="panorama.open({&#10;    x:$0$,&#10;    y:$01$,&#10;    w:$320$,&#10;    h:$300$,&#10;    imgPath:'$imgPath$'&#10;},function(ret,err){&#10;    &#10;});" description="打开全景展示视图" toReformat="true" toShortenFQNames="true">
    <variable name="0" expression="snakeCase(&quot;0&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="01" expression="snakeCase(&quot;0&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="320" expression="snakeCase(&quot;320&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="300" expression="snakeCase(&quot;300&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="imgPath" expression="snakeCase(&quot;imgpath&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="panorama.close" value="panorama.close({&#10;    id:$id$&#10;});" description="关闭全景展示视图" toReformat="true" toShortenFQNames="true">
    <variable name="id" expression="snakeCase(&quot;id&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="panorama.hide" value="panorama.hide({&#10;    id:$id$&#10;});" description="隐藏全景展示视图" toReformat="true" toShortenFQNames="true">
    <variable name="id" expression="snakeCase(&quot;id&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="panorama.show" value="panorama.show({&#10;    id:$id$&#10;});" description="显示全景展示视图" toReformat="true" toShortenFQNames="true">
    <variable name="id" expression="snakeCase(&quot;id&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>