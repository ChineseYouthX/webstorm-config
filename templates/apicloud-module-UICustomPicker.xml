  </template>
  <template name="UICustomPicker.setValue" value="UICustomPicker.setValue({&#10;     id:$id$,&#10;     data:$dataArray$&#10;});" description="设置自定义选择器当前选中的内容" toReformat="true" toShortenFQNames="true">
