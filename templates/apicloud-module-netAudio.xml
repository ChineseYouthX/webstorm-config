<templateSet group="apicloud-module-netAudio">
  <template name="netAudio.play" value="netAudio.play({&#10;    path: '$path$'//网络音频资源地址&#10;}, function(ret,err){&#10;    &#10;});" description="播放网络音频" toReformat="true" toShortenFQNames="true">
    <variable name="path" expression="snakeCase(&quot;path&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="netAudio.setVolume" value="netAudio.setVolume({&#10;    volume: $0$&#10;});" description="设置音量" toReformat="true" toShortenFQNames="true">
    <variable name="0" expression="snakeCase(&quot;0&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="netAudio.setProgress" value="netAudio.setProgress({&#10;    progress: $0$&#10;});" description="设置播放位置" toReformat="true" toShortenFQNames="true">
    <variable name="0" expression="snakeCase(&quot;0&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="netAudio.pause" value="netAudio.pause();" description="暂停播放" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="netAudio.stop" value="netAudio.stop();" description="停止播放" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>