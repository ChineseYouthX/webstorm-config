<templateSet group="apicloud-module-unionPay">
  <template name="unPay.pay" value="unPay.pay({&#10;    tn: '$tn$',&#10;    devMode: $true$&#10;}, function(ret, err){&#10;&#10;});" description="按交易流水号支付订单" toReformat="true" toShortenFQNames="true">
